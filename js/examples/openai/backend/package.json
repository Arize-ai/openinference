{
  "name": "arize-openai-express",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsup ./index.ts ./instrumentation2.ts --format cjs --dts",
    "start": "node --import ./dist/instrumentation2.cjs ./dist/index.cjs",
    "dev": "concurrently \"tsup ./index.ts ./instrumentation2.ts --format cjs --dts --watch\" \"nodemon --inspect --import ./dist/instrumentation2.cjs -q ./dist/index.cjs\"",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@arizeai/openinference-instrumentation-openai": "^0.5.0",
    "@grpc/grpc-js": "^1.11.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.53.0",
    "@opentelemetry/instrumentation": "^0.53.0",
    "@opentelemetry/resources": "^1.26.0",
    "@opentelemetry/sdk-trace-base": "^1.26.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "axios": "^1.7.4",
    "node-emoji": "2.1.3",
    "openai": "^4.53.2"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/aws-lambda": "^8.10.143",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^28.1.7",
    "@types/node": "<19.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "constructs": "10.3.0",
    "datadog-cdk-constructs-v2": "^1.14.1",
    "datadog-lambda-js": "^9.115.0",
    "dd-trace": "^5.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-n": "^17.9.0",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-sonarjs": "^1.0.4-alpha.1",
    "husky": "^8.0.3",
    "jest": "^29.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sst": "2.43.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.1.6",
    "vitest": "^2.0.5"
  }
}
